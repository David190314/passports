        <input type="password" class="form-control" name="password">
      </div>
    </div>
    <div class="col-md-12 d-flex justify-content-center  mb-5 mt-4">
        <button type="submit" class="btn btn-primary border-dark">Submit</button>
        <button type="reset" class="btn btn-secondary  border-dark ms-4">Reset</button>
